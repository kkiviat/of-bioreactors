FoamFile
{
    format      ascii;
    class       volScalarField;
    object      km;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 3.0029232467417088e-05;

boundaryField
{
    outlet
    {
        type            inletOutlet;
        phi             phim;
        inletValue      uniform 6.7335e-11;
        value           uniform 6.7335e-11;
    }
    wall
    {
        type            zeroGradient;
        value           $internalField;
    }
    sparger_upper
    {
        type            fixedValue;
        value           $internalField;
    }
}

