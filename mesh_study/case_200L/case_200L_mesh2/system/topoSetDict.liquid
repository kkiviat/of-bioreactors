actions
(
/** ============ **/
/**  liquid      **/
/** ============ **/
{
    name        liquidCellSet;
    type        cellSet;
    action      new;

    source      cylinderToCell;
    sourceInfo
    {
        point1          ( 0 0 0 );
        point2          ( 0 0.8674201212255561 0 );
        radius          0.2710687878829863;
    }
}
// Convert cellSet to cellZone
{
    name        liquid;
    type        cellZoneSet;
    action      new;

    source setToCellZone;
    sourceInfo
    {
        cellSet liquidCellSet;
    }
}

);

